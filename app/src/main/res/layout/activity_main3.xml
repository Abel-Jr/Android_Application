<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity3">

    <Button
        android:id="@+id/supprimer_annonce"
        android:layout_width="249dp"
        android:layout_height="28dp"
        android:layout_marginEnd="76dp"
        android:layout_marginRight="76dp"
        android:layout_marginBottom="52dp"
        android:background="#FFFFFF"
        android:text="Delete an announcement"
        android:textColor="#201F1F"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/modifier_profil"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/Consulter_annonce"
        android:layout_width="243dp"
        android:layout_height="31dp"
        android:layout_marginEnd="76dp"
        android:layout_marginRight="76dp"
        android:layout_marginBottom="44dp"
        android:background="#FFFFFF"
        android:text="Consult an annoucement"
        android:textColor="#171515"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/supprimer_annonce"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/up"
        android:layout_width="266dp"
        android:layout_height="38dp"
        android:layout_marginEnd="64dp"
        android:layout_marginRight="64dp"
        android:layout_marginBottom="84dp"
        android:text="Welcome"
        android:textColor="#1E1D1D"
        android:textColorHint="#333030"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/ajouter_anonce"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/Lerror"
        android:layout_width="246dp"
        android:layout_height="20dp"
        android:layout_marginEnd="91dp"
        android:layout_marginRight="91dp"
        android:layout_marginBottom="53dp"
        android:text="TextView"
        android:textColor="#AA0C0C"
        android:textSize="14sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/button_retour"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/button_retour"
        android:layout_width="110dp"
        android:layout_height="46dp"
        android:layout_marginEnd="168dp"
        android:layout_marginRight="168dp"
        android:layout_marginBottom="32dp"
        android:background="#283549"
        android:text="Back"
        android:textColor="#F3EEEE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/ajouter_anonce"
        android:layout_width="238dp"
        android:layout_height="31dp"
        android:layout_marginEnd="80dp"
        android:layout_marginRight="80dp"
        android:layout_marginBottom="52dp"
        android:background="#FFFFFF"
        android:text="Add an announcement"
        android:textColor="#171515"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Consulter_annonce"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/modifier_profil"
        android:layout_width="255dp"
        android:layout_height="31dp"
        android:layout_marginEnd="76dp"
        android:layout_marginRight="76dp"
        android:layout_marginBottom="48dp"
        android:background="#FFFFFF"
        android:text="Edit your profile"
        android:textColor="#1C1B1B"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Lerror"
        app:layout_constraintEnd_toEndOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>